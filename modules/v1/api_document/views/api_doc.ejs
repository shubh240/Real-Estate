                top: 0;
                z-index: 999;
                width: 100%;
                height: 100%;
                overflow: visible;
                background: #fff url('<%= GLOBALS.BASE_URL_WITHOUT_API + GLOBALS.LOADER_IMAGE %>') no-repeat center center;
